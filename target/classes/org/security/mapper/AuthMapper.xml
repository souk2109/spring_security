<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.security.mapper.AuthMapper">
	<insert id="insert">
		insert into tbl_user_auth(id, auth)
		values(#{id}, 'ROLE_USER')
	</insert>

</mapper>

